<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:scrollbars="none"
    android:orientation="vertical"
    tools:context="sg.gov.msf.bbss.view">

    <!-- Select Type -->
    <include
        android:id="@+id/section_new_type_header"
        layout="@layout/section_header_row"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" />

    <include
        android:id="@+id/services_change_reason"
        layout="@layout/show_label_spinner_row"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" />

    <include
        android:id="@+id/services_change_reason_other"
        layout="@layout/show_label_edittext_row"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"/>

    <include
        android:id="@+id/services_new_type"
        layout="@layout/show_label_spinner_row"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" />

</LinearLayout>
